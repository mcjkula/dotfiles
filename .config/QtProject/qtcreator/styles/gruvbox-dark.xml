<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Gruvbox - Dark">
  <style name="Text" foreground="#(light1)" background="#(dark0)"/>
  <style name="Link" foreground="#(light4)"/>
  <style name="Selection" foreground="#(dark3)" background="#(light1)"/>
  <style name="LineNumber" foreground="#(dark4)"/>
  <style name="SearchResult" foreground="#(dark0)" background="#(bright_yellow)"/>
  <style name="SearchScope" foreground="#(light1)"/>
  <style name="Parentheses" foreground="#(light1)" background="#(dark3)" bold="true"/>
  <style name="CurrentLine" foreground="#(light1)" background="#(dark1)"/>
  <style name="CurrentLineNumber" foreground="#(bright_yellow)" background="#(dark1)"/>
  <style name="Occurrences" foreground="#(light1)" background="#(dark3)" bold="true"/>
  <style name="Occurrences.Unused" foreground="#(light1)"/>
  <style name="Occurrences.Rename" foreground="#(light1)"/>
  <style name="Number" foreground="#(bright_purple)"/>
  <style name="String" foreground="#(bright_green)"/>
  <style name="Type" foreground="#(bright_yellow)"/>
  <style name="Local" foreground="#(light1)"/>
  <style name="Field" foreground="#(bright_blue)"/>
  <style name="Static" foreground="#(bright_purple)"/>
  <style name="VirtualMethod" foreground="#(bright_green)" bold="true" italic="true"/>
  <style name="Function" foreground="#(bright_green)" bold="true"/>
  <style name="Keyword" foreground="#(bright_red)"/>
  <style name="Operator" foreground="#(light1)"/>
  <style name="Preprocessor" foreground="#(bright_aqua)"/>
  <style name="Label" foreground="#(bright_red)"/>
  <style name="Comment" foreground="#(gray_245)" italic="true"/>
  <style name="Doxygen.Comment" foreground="#(bright_orange)"/>
  <style name="Doxygen.Tag" foreground="#(light1)" bold="true"/>
  <style name="VisualWhitespace" foreground="#(dark1)"/>
  <style name="QmlLocalId" foreground="#(bright_green)" bold="true" italic="true"/>
  <style name="QmlExternalId" foreground="#(bright_purple)" italic="true"/>
  <style name="QmlTypeId" foreground="#(bright_yellow)"/>
  <style name="QmlRootObjectProperty" foreground="#(bright_green)" bold="true" italic="true"/>
  <style name="QmlScopeObjectProperty" foreground="#(bright_green)" bold="true" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#(bright_purple)" italic="true"/>
  <style name="JsScopeVar" foreground="#(light1)" italic="true"/>
  <style name="JsImportVar" foreground="#(light1)" italic="true"/>
  <style name="JsGlobalVar" foreground="#(light1)" italic="true"/>
  <style name="QmlStateName" foreground="#(bright_green)" bold="true"/>
  <style name="Binding" foreground="#(bright_red)"/>
  <style name="DisabledCode" foreground="#(dark4)"/>
  <style name="AddedLine" foreground="#(dark0)" background="#(bright_green)"/>
  <style name="RemovedLine" foreground="#(dark0)" background="#(bright_red)"/>
  <style name="DiffFile" foreground="#(dark0)" background="#(bright_yellow)"/>
  <style name="DiffLocation" foreground="#(dark0)" background="#(bright_yellow)"/>
  <style name="DiffFileLine" foreground="#(gray_245)" background="#(dark1)" italic="true"/>
  <style name="DiffContextLine" foreground="#(gray_245)" background="#(dark1)" italic="true"/>
  <style name="DiffSourceLine" foreground="#(dark0)" background="#(bright_aqua)"/>
  <style name="DiffSourceChar" foreground="#(dark0)" background="#(bright_red)"/>
  <style name="DiffDestLine" foreground="#(dark0)" background="#(bright_aqua)"/>
  <style name="DiffDestChar" foreground="#(dark0)" background="#(bright_green)"/>
</style-scheme>
